// Place your settings in this file to overwrite the default settings
{
  "editor.fontFamily": "Source Code Pro for Powerline, Menlo",
  "editor.fontWeight": "100",
  "editor.fontSize": 14,
  "editor.minimap.renderCharacters": false,
  "editor.fontLigatures": true,
  "explorer.confirmDragAndDrop": false,
  "explorer.decorations.badges": false,
  "files.exclude": {
    "**/.git": true,
    "**/.gradle": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "http.proxyStrictSSL": false,
  "gitlens.blame.avatars": false,
  "gitlens.keymap": "alternate",
  "gitlens.statusBar.enabled": false,
  "gitlens.codeLens.recentChange.enabled": false,
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.blame.format": "${author|20?} @ ${id} ${ago|14-}",
  "vim.useSystemClipboard": true,
  "vim.leader": "<space>",
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "k"],
      "after": ["<Esc>"]
    }
  ],
  "workbench.colorTheme": "Community Material Theme",
  "gitlens.advanced.messages": {
    "suppressShowKeyBindingsNotice": true
  },
  "files.insertFinalNewline": true,
  "typescript.updateImportsOnFileMove.enabled": "never",
  "breadcrumbs.enabled": true,
  "debug.node.autoAttach": "on",
  "gitlens.views.fileHistory.enabled": true,
  "gitlens.views.fileHistory.location": "scm",
  "gitlens.views.lineHistory.enabled": true,
  "gitlens.views.lineHistory.location": "scm",
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "explorer.openEditors.visible": 0,
  "editor.minimap.showSlider": "always",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorBlinking": "phase",
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "gitlens.advanced.telemetry.enabled": false,
  "workbench.settings.enableNaturalLanguageSearch": false,
  "openInGitHub.defaultBranch": "develop",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.iconTheme": "eq-material-theme-icons",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "python.jediEnabled": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "workbench.editor.wrapTabs": true,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "editor.tabSize": 8,
  "editor.cursorStyle": "line",
  "editor.insertSpaces": false,
  "editor.lineNumbers": "on",
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "editor.wordWrap": "off",
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
"python.languageServer": "Default",
"gitlens.hovers.currentLine.over": "line",
"gitlens.currentLine.enabled": false,
"gitlens.codeLens.enabled": false,
"snyk.yesWelcomeNotification": false,
"editor.stickyScroll.enabled": true,
"window.zoomLevel": 3
}
